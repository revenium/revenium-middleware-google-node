  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revenium/revenium-middleware-google-node.git"
  },
  "homepage": "https://github.com/revenium/revenium-middleware-google-node#readme",
  "bugs": {
    "url": "https://github.com/revenium/revenium-middleware-google-node/issues"
  },
  "dependencies": {
    "@google/genai": "^1.18.0",
    "@revenium/google-core": "^0.1.2"
  },
  "devDependencies": {
    "tsx": "^4.20.6"
