{
  "name": "@revenium/google-genai",
  "version": "0.1.3",
  "description": "Transparent TypeScript middleware for automatic Revenium usage tracking with Google AI (Gemini)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
